<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#eee">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#bbb"
                android:dashWidth="4dp"
                android:dashGap="4dp" />
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
        </shape>
    </item>
</ripple>
